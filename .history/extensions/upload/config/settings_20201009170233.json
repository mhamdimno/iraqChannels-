{
    "provider": "aws-s3-sharp-resize",
    "providerOptions": {
      "accessKeyId": "${process.env.ACCESS_KEY_ID}",
      "secretAccessKey": "${process.env.SECRET_ACCESS_KEY}",
      "endpoint": "nyc3.digitaloceanspaces.com",
      "optimize": {
        "resize": true,
        "width": 1000,
        "height": 1000
      },
      "settings": {
        "awsUploadFolder": "your upload folder"
      },
      "params": {
        "Bucket": "strapi-app"
      }
    }
  }